    <xs:element ref='rib:pointsgeneralpolygons'/>
    <xs:element ref='rib:basis'/>
    <xs:element ref='rib:patch'/>
    <xs:element ref='rib:patchmesh'/>
    <xs:element ref='rib:nupatch'/>
    <xs:element ref='rib:trimcurve'/>
    <xs:element ref='rib:subdivisionmesh'/>
    <xs:element ref='rib:sphere'/>
    <xs:element ref='rib:cone'/>
    <xs:element ref='rib:cylinder'/>
    <xs:element ref='rib:hyperboloid'/>
    <xs:element ref='rib:paraboloid'/>
    <xs:element ref='rib:disk'/>
    <xs:element ref='rib:torus'/>
    <xs:element ref='rib:points'/>
    <xs:element ref='rib:curves'/>
    <xs:element ref='rib:blobby'/>
    <xs:element ref='rib:procedural'/>
    <xs:element ref='rib:geometry'/>
    <xs:element ref='rib:solid'/>
    <xs:element ref='rib:object'/>
    <xs:element ref='rib:maketexture'/>
    <xs:element ref='rib:makelatlongenvironment'/>
    <xs:element ref='rib:makecubefaceenvironment'/>
    <xs:element ref='rib:makeshadow'/>
    <xs:element ref='rib:archiverecord'/>
    <xs:element ref='rib:readarchive'/>
   </xs:sequence>
  </xs:complexType>
 </xs:element>

 <xs:element name='float'>
  <xs:complexType>
   <xs:attribute name='name' type='xs:string' use='optional'/>
   <xs:attribute name='value' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='array'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:row'/>
   </xs:sequence>
   <xs:attribute name='xlength' type='xs:string' use='optional'/>
   <xs:attribute name='ylength' type='xs:string' use='optional'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='rtmatrix'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:row'/>
   </xs:sequence>
   <xs:attribute name='xlength' type='xs:string' use='optional'/>
   <xs:attribute name='ylength' type='xs:string' use='optional'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='row'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:col'/>
   </xs:sequence>
  </xs:complexType>
 </xs:element>

 <xs:element name='col'>
  <xs:complexType mixed='true'>
  </xs:complexType>
 </xs:element>

 <xs:element name='rtpoint'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:float'/>
   </xs:sequence>
  </xs:complexType>
 </xs:element>

 <xs:element name='rtbound'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:float'/>
   </xs:sequence>
  </xs:complexType>
 </xs:element>

 <xs:element name='declare'>
  <xs:complexType>
   <xs:attribute name='name' type='xs:string' use='required'/>
   <xs:attribute name='class' use='optional'>
    <xs:simpleType>
     <xs:restriction base='xs:string'>
      <xs:enumeration value='constant'/>
      <xs:enumeration value='uniform'/>
      <xs:enumeration value='varying'/>
      <xs:enumeration value='vertex'/>
     </xs:restriction>
    </xs:simpleType>
   </xs:attribute>
   <xs:attribute name='type' use='optional'>
    <xs:simpleType>
     <xs:restriction base='xs:string'>
      <xs:enumeration value='float'/>
      <xs:enumeration value='integer'/>
      <xs:enumeration value='string'/>
      <xs:enumeration value='color'/>
      <xs:enumeration value='point'/>
      <xs:enumeration value='vector'/>
      <xs:enumeration value='normal'/>
      <xs:enumeration value='rtmatrix'/>
      <xs:enumeration value='hpoint'/>
     </xs:restriction>
    </xs:simpleType>
   </xs:attribute>
   <xs:attribute name='dimension' type='xs:string' default='1'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='param'>
  <xs:complexType>
   <xs:attribute name='name' type='xs:string' use='required'/>
   <xs:attribute name='value' type='xs:string' use='optional'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='frame'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:frame'/>
    <xs:element ref='rib:world'/>
    <xs:element ref='rib:format'/>
    <xs:element ref='rib:frameaspectratio'/>
    <xs:element ref='rib:screenwindow'/>
    <xs:element ref='rib:cropwindow'/>
    <xs:element ref='rib:projection'/>
    <xs:element ref='rib:clipping'/>
    <xs:element ref='rib:clippingplane'/>
    <xs:element ref='rib:depthoffield'/>
    <xs:element ref='rib:shutter'/>
    <xs:element ref='rib:pixelvariance'/>
    <xs:element ref='rib:pixelsamples'/>
    <xs:element ref='rib:pixelfilter'/>
    <xs:element ref='rib:exposure'/>
    <xs:element ref='rib:imager'/>
    <xs:element ref='rib:quantize'/>
    <xs:element ref='rib:display'/>
    <xs:element ref='rib:hider'/>
    <xs:element ref='rib:colorsamples'/>
    <xs:element ref='rib:relativedetail'/>
    <xs:element ref='rib:attribute'/>
    <xs:element ref='rib:color'/>
    <xs:element ref='rib:opacity'/>
    <xs:element ref='rib:texturecoordinates'/>
    <xs:element ref='rib:lightsource'/>
    <xs:element ref='rib:arealightsource'/>
    <xs:element ref='rib:illuminate'/>
    <xs:element ref='rib:surface'/>
    <xs:element ref='rib:displacement'/>
    <xs:element ref='rib:atmosphere'/>
    <xs:element ref='rib:interior'/>
    <xs:element ref='rib:exterior'/>
    <xs:element ref='rib:shadingrate'/>
    <xs:element ref='rib:shadinginterpolation'/>
    <xs:element ref='rib:matte'/>
    <xs:element ref='rib:bound'/>
    <xs:element ref='rib:detail'/>
    <xs:element ref='rib:detailrange'/>
    <xs:element ref='rib:geometricapproximation'/>
    <xs:element ref='rib:orientation'/>
    <xs:element ref='rib:reverseorientation'/>
    <xs:element ref='rib:sides'/>
    <xs:element ref='rib:identity'/>
    <xs:element ref='rib:transform'/>
    <xs:element ref='rib:concattransform'/>
    <xs:element ref='rib:perspective'/>
    <xs:element ref='rib:translate'/>
    <xs:element ref='rib:rotate'/>
    <xs:element ref='rib:scale'/>
    <xs:element ref='rib:skew'/>
    <xs:element ref='rib:coordinatesystem'/>
    <xs:element ref='rib:coordsystransform'/>
    <xs:element ref='rib:transformpoints'/>
    <xs:element ref='rib:transform'/>
    <xs:element ref='rib:attribute'/>
    <xs:element ref='rib:world'/>
   </xs:sequence>
   <xs:attribute name='nr' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='world'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:polygon'/>
    <xs:element ref='rib:generalpolygon'/>
    <xs:element ref='rib:pointspolygons'/>
    <xs:element ref='rib:pointsgeneralpolygons'/>
    <xs:element ref='rib:basis'/>
    <xs:element ref='rib:patch'/>
    <xs:element ref='rib:patchmesh'/>
    <xs:element ref='rib:nupatch'/>
    <xs:element ref='rib:trimcurve'/>
    <xs:element ref='rib:subdivisionmesh'/>
    <xs:element ref='rib:sphere'/>
    <xs:element ref='rib:cone'/>
    <xs:element ref='rib:cylinder'/>
    <xs:element ref='rib:hyperboloid'/>
    <xs:element ref='rib:paraboloid'/>
    <xs:element ref='rib:disk'/>
    <xs:element ref='rib:torus'/>
    <xs:element ref='rib:points'/>
    <xs:element ref='rib:curves'/>
    <xs:element ref='rib:blobby'/>
    <xs:element ref='rib:procedural'/>
    <xs:element ref='rib:geometry'/>
    <xs:element ref='rib:solid'/>
    <xs:element ref='rib:object'/>
    <xs:element ref='rib:attribute'/>
    <xs:element ref='rib:color'/>
    <xs:element ref='rib:opacity'/>
    <xs:element ref='rib:texturecoordinates'/>
    <xs:element ref='rib:lightsource'/>
    <xs:element ref='rib:arealightsource'/>
    <xs:element ref='rib:illuminate'/>
    <xs:element ref='rib:surface'/>
    <xs:element ref='rib:displacement'/>
    <xs:element ref='rib:atmosphere'/>
    <xs:element ref='rib:interior'/>
    <xs:element ref='rib:exterior'/>
    <xs:element ref='rib:shadingrate'/>
    <xs:element ref='rib:shadinginterpolation'/>
    <xs:element ref='rib:matte'/>
    <xs:element ref='rib:bound'/>
    <xs:element ref='rib:detail'/>
    <xs:element ref='rib:detailrange'/>
    <xs:element ref='rib:geometricapproximation'/>
    <xs:element ref='rib:orientation'/>
    <xs:element ref='rib:reverseorientation'/>
    <xs:element ref='rib:sides'/>
    <xs:element ref='rib:identity'/>
    <xs:element ref='rib:transform'/>
    <xs:element ref='rib:concattransform'/>
    <xs:element ref='rib:perspective'/>
    <xs:element ref='rib:translate'/>
    <xs:element ref='rib:rotate'/>
    <xs:element ref='rib:scale'/>
    <xs:element ref='rib:skew'/>
    <xs:element ref='rib:coordinatesystem'/>
    <xs:element ref='rib:coordsystransform'/>
    <xs:element ref='rib:transformpoints'/>
    <xs:element ref='rib:transform'/>
    <xs:element ref='rib:motion'/>
    <xs:element ref='rib:maketexture'/>
    <xs:element ref='rib:makelatlongenvironment'/>
    <xs:element ref='rib:makecubefaceenvironment'/>
    <xs:element ref='rib:makeshadow'/>
    <xs:element ref='rib:archiverecord'/>
    <xs:element ref='rib:readarchive'/>
   </xs:sequence>
  </xs:complexType>
 </xs:element>

 <xs:element name='format'>
  <xs:complexType>
   <xs:attribute name='xresolution' type='xs:string' use='required'/>
   <xs:attribute name='yresolution' type='xs:string' use='required'/>
   <xs:attribute name='pixelaspectratio' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='frameaspectratio'>
  <xs:complexType/>
 </xs:element>

 <xs:element name='screenwindow'>
  <xs:complexType>
   <xs:attribute name='left' type='xs:string' use='required'/>
   <xs:attribute name='right' type='xs:string' use='required'/>
   <xs:attribute name='bottom' type='xs:string' use='required'/>
   <xs:attribute name='top' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='cropwindow'>
  <xs:complexType>
   <xs:attribute name='xmin' type='xs:string' use='required'/>
   <xs:attribute name='xmax' type='xs:string' use='required'/>
   <xs:attribute name='ymin' type='xs:string' use='required'/>
   <xs:attribute name='ymax' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='projection'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:param'/>
   </xs:sequence>
   <xs:attribute name='name' use='required'>
    <xs:simpleType>
     <xs:restriction base='xs:string'>
      <xs:enumeration value='perspective'/>
      <xs:enumeration value='orthographic'/>
      <xs:enumeration value='null'/>
     </xs:restriction>
    </xs:simpleType>
   </xs:attribute>
  </xs:complexType>
 </xs:element>

 <xs:element name='clipping'>
  <xs:complexType>
   <xs:attribute name='near' type='xs:string' use='required'/>
   <xs:attribute name='far' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='clippingplane'>
  <xs:complexType>
   <xs:attribute name='x' type='xs:string' use='required'/>
   <xs:attribute name='y' type='xs:string' use='required'/>
   <xs:attribute name='z' type='xs:string' use='required'/>
   <xs:attribute name='nx' type='xs:string' use='required'/>
   <xs:attribute name='ny' type='xs:string' use='required'/>
   <xs:attribute name='nz' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='depthoffield'>
  <xs:complexType>
   <xs:attribute name='fstop' type='xs:string' use='required'/>
   <xs:attribute name='focallength' type='xs:string' use='required'/>
   <xs:attribute name='focaldistance' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='shutter'>
  <xs:complexType>
   <xs:attribute name='min' type='xs:string' use='required'/>
   <xs:attribute name='max' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='pixelvariance'>
  <xs:complexType>
   <xs:attribute name='variation' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='pixelsamples'>
  <xs:complexType>
   <xs:attribute name='xsamples' type='xs:string' use='required'/>
   <xs:attribute name='ysamples' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='pixelfilter'>
  <xs:complexType>
   <xs:attribute name='filterfunc' use='required'>
    <xs:simpleType>
     <xs:restriction base='xs:string'>
      <xs:enumeration value='box'/>
      <xs:enumeration value='triangle'/>
      <xs:enumeration value='catmullrom'/>
      <xs:enumeration value='guassian'/>
      <xs:enumeration value='sinc'/>
      <xs:enumeration value='CDATA'/>
     </xs:restriction>
    </xs:simpleType>
   </xs:attribute>
   <xs:attribute name='xwidth' type='xs:string' use='required'/>
   <xs:attribute name='ywidth' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='exposure'>
  <xs:complexType>
   <xs:attribute name='gain' type='xs:string' use='required'/>
   <xs:attribute name='gamma' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='imager'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:param'/>
   </xs:sequence>
   <xs:attribute name='name' use='required'>
    <xs:simpleType>
     <xs:restriction base='xs:string'>
      <xs:enumeration value='CDATA'/>
      <xs:enumeration value='null'/>
     </xs:restriction>
    </xs:simpleType>
   </xs:attribute>
  </xs:complexType>
 </xs:element>

 <xs:element name='quantize'>
  <xs:complexType>
   <xs:attribute name='type' use='required'>
    <xs:simpleType>
     <xs:restriction base='xs:string'>
      <xs:enumeration value='rgba'/>
      <xs:enumeration value='z'/>
     </xs:restriction>
    </xs:simpleType>
   </xs:attribute>
   <xs:attribute name='one' type='xs:string' use='required'/>
   <xs:attribute name='min' type='xs:string' use='required'/>
   <xs:attribute name='max' type='xs:string' use='required'/>
   <xs:attribute name='ditheramplitude' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='display'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:param'/>
   </xs:sequence>
   <xs:attribute name='name' type='xs:string' use='required'/>
   <xs:attribute name='type' type='xs:string' use='required'/>
   <xs:attribute name='mode' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='hider'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:param'/>
   </xs:sequence>
   <xs:attribute name='type' use='required'>
    <xs:simpleType>
     <xs:restriction base='xs:string'>
      <xs:enumeration value='hidden'/>
      <xs:enumeration value='null'/>
      <xs:enumeration value='CDATA'/>
     </xs:restriction>
    </xs:simpleType>
   </xs:attribute>
  </xs:complexType>
 </xs:element>

 <xs:element name='colorsamples'>
  <xs:complexType>
   <xs:choice>
    <xs:element ref='rib:nrgb'/>
    <xs:element ref='rib:rgbn'/>
   </xs:choice>
   <xs:attribute name='n' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='nrgb'>
  <xs:complexType>
   <xs:attribute name='n0' type='xs:string' use='required'/>
   <xs:attribute name='n1' type='xs:string' use='required'/>
   <xs:attribute name='n2' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='rgbn'>
  <xs:complexType>
   <xs:attribute name='n0' type='xs:string' use='required'/>
   <xs:attribute name='n1' type='xs:string' use='required'/>
   <xs:attribute name='n2' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='relativedetail'>
  <xs:complexType>
   <xs:attribute name='relativedetail' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='option'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:param'/>
   </xs:sequence>
   <xs:attribute name='name' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='attributestack'>
  <xs:complexType>
   <xs:sequence>
    <xs:element ref='rib:attribute'/>
    <xs:element ref='rib:color'/>
    <xs:element ref='rib:opacity'/>
    <xs:element ref='rib:texturecoordinates'/>
    <xs:element ref='rib:lightsource'/>
    <xs:element ref='rib:arealightsource'/>
    <xs:element ref='rib:illuminate'/>
    <xs:element ref='rib:surface'/>
    <xs:element ref='rib:displacement'/>
    <xs:element ref='rib:atmosphere'/>
    <xs:element ref='rib:interior'/>
    <xs:element ref='rib:exterior'/>
    <xs:element ref='rib:shadingrate'/>
    <xs:element ref='rib:shadinginterpolation'/>
    <xs:element ref='rib:matte'/>
    <xs:element ref='rib:bound'/>
    <xs:element ref='rib:detail'/>
    <xs:element ref='rib:detailrange'/>
    <xs:element ref='rib:geometricapproximation'/>
    <xs:element ref='rib:orientation'/>
    <xs:element ref='rib:reverseorientation'/>
    <xs:element ref='rib:sides'/>
   </xs:sequence>
  </xs:complexType>
 </xs:element>

 <xs:element name='color'>
  <xs:complexType>
   <xs:attribute name='r' type='xs:string' use='required'/>
   <xs:attribute name='g' type='xs:string' use='required'/>
   <xs:attribute name='b' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='opacity'>
  <xs:complexType>
   <xs:attribute name='r' type='xs:string' use='required'/>
   <xs:attribute name='g' type='xs:string' use='required'/>
   <xs:attribute name='b' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='texturecoordinates'>
  <xs:complexType>
   <xs:attribute name='s1' type='xs:string' use='required'/>
   <xs:attribute name='t1' type='xs:string' use='required'/>
   <xs:attribute name='s2' type='xs:string' use='required'/>
   <xs:attribute name='t2' type='xs:string' use='required'/>
   <xs:attribute name='s3' type='xs:string' use='required'/>
   <xs:attribute name='t3' type='xs:string' use='required'/>
   <xs:attribute name='s4' type='xs:string' use='required'/>
   <xs:attribute name='t4' type='xs:string' use='required'/>
  </xs:complexType>
 </xs:element>

 <xs:element name='lightsource'>
  <xs:complexType>
   <xs:sequence minOccurs='0' maxOccurs='unbounded'>
    <xs:element ref='rib:param'/>
   </xs:sequence>
   <xs:attribute name='shadername' use='required'>
    <xs:simpleType>
     <xs:restriction base='xs:string'>
      <xs:enumeration value='ambient'/>
      <xs:enumeration value='distant'/>
      <xs:enumeration value='point'/>
      <xs:enumeration value='spot'/>
      <xs:enumeration value='CDATA'/>
     </xs:restriction>
    </xs:simpleType>
   </xs:attribute>
  </xs:complexType>
