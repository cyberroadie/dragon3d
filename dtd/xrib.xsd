<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
 xmlns:rib="http://www.transformatorhuis.com/rib/"
 xmlns:xs="http://www.w3.org/2001/XMLSchema"
 targetNamespace="http://www.transformatorhuis.com/rib/">

 <xs:element name='rib'>
  <xs:complexType>
   <xs:sequence>
    <xs:element ref='rib:frame'/>
    <xs:element ref='rib:world'/>
