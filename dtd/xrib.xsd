    <xs:element ref='rib:reverseorientation'/>
    <xs:element ref='rib:sides'/>
    <xs:element ref='rib:identity'/>
    <xs:element ref='rib:transform'/>
    <xs:element ref='rib:concattransform'/>
    <xs:element ref='rib:perspective'/>
    <xs:element ref='rib:translate'/>
    <xs:element ref='rib:rotate'/>
    <xs:element ref='rib:scale'/>
    <xs:element ref='rib:skew'/>
    <xs:element ref='rib:coordinatesystem'/>
    <xs:element ref='rib:coordsystransform'/>
    <xs:element ref='rib:transformpoints'/>
