    <xs:element ref='rib:imager'/>
    <xs:element ref='rib:quantize'/>
    <xs:element ref='rib:display'/>
    <xs:element ref='rib:hider'/>
    <xs:element ref='rib:colorsamples'/>
    <xs:element ref='rib:relativedetail'/>
    <xs:element ref='rib:attribute'/>
    <xs:element ref='rib:color'/>
    <xs:element ref='rib:opacity'/>
    <xs:element ref='rib:texturecoordinates'/>
    <xs:element ref='rib:lightsource'/>
    <xs:element ref='rib:arealightsource'/>
    <xs:element ref='rib:illuminate'/>
